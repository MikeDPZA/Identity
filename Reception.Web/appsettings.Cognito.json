{
  "Provider": "Cognito",
  "CognitoConfiguration": {
    "Domain": "some-domain",
    "UserPoolId": "some-userpool-id",
    "Region": "some-region",
    "UserCredentialsFlow": {
      "RedirectUri": "https://some-host/v1/Token/Authorization",
      "ClientId": "some-uc-client-id",
      "ClientSecret": "some-uc-client-secret",
      "Scopes": ["some-scope"]
    },
    "ClientCredentialsFlow": {
      "ClientId": "some-cc-client-id",
      "ClientSecret": "some-cc-client-secret",
      "Scopes": ["some-scope"]
    }
  }
}